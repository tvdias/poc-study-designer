          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Pick Random Project ID" enabled="true">
            <stringProp name="scriptLanguage">java</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script"><![CDATA[
String countStr = vars.get("project_id_matchNr");
int count = countStr != null ? Integer.parseInt(countStr) : 0;
if (count > 0) {
    int randomProjectIndex = 1 + (int)(Math.random() * count);
    vars.put("project_id", vars.get("project_id_" + randomProjectIndex));
} else {
    vars.put("project_id", "NO_PROJECT_ID");
}
]]></stringProp>
