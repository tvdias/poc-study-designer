          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Handle 400 - Already Exists">
            <stringProp name="scriptLanguage">java</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script"><![CDATA[
String code = prev.getResponseCode();
String body = prev.getResponseDataAsString();
if (code.equals("400") && body.contains("A question already exists on the project with the same variable name")) {
    AssertionResult.setFailure(false);
    prev.setSuccessful(true);
} else if (!code.equals("200")) {
    AssertionResult.setFailure(true);
    AssertionResult.setFailureMessage("Unexpected response code: " + code);
}
            ]]></stringProp>
