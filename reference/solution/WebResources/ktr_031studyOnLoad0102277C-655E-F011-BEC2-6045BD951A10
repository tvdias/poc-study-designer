/**
 * @file        031-studyOnLoad.js
 * @description Executes on form load
 *
 * @date        2025-07-11
 * @version     1.1
 *
 * @usage       This script is invoked on load of the Study Main form of Study entity
 * @notes       
 */

const Draft = 1; // Your status reason value

function disableFWLSubgridButtons(formContext) {
    
    var statusAttr = formContext.getAttribute("statuscode").getValue();
    
    // If status code is NOT Draft, disable Fieldwork Market Languages grid buttons
    if (statusAttr !== Draft) {
        return false;
        }
    else {
        return true;
    }
}

